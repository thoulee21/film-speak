{
  "expo": {
    "name": "FilmSpeak",
    "slug": "film-speak",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2001000"
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.thoulee.filmspeak",
      "versionCode": 2001000,
      "intentFilters": [
        {
          "action": "SEND",
          "category": "DEFAULT",
          "data": {
            "mimeType": "video/*"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./plugin/gradle-properties",
      "expo-router",
      [
        "react-native-video",
        {
          "enableNotificationControls": true,
          "enableBackgroundAudio": false,
          "enableADSExtension": true,
          "enableCacheExtension": false,
          "enableAndroidPictureInPicture": true,
          "androidExtensions": {
            "useExoplayerRtsp": true,
            "useExoplayerSmoothStreaming": true,
            "useExoplayerHls": true,
            "useExoplayerDash": true,
            "useExoplayerIMA": true
          }
        }
      ],
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enableProguardInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6c832a57-bb37-4208-9672-223a964aaa88"
      }
    },
    "owner": "thoulee",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6c832a57-bb37-4208-9672-223a964aaa88",
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    }
  }
}