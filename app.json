{
  "expo": {
    "name": "FilmSpeak",
    "slug": "film-speak",
    "version": "1.3.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1003002"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thoulee.filmspeak",
      "versionCode": 1003002
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./plugin/gradle-properties",
      "./plugin/android-manifest",
      "expo-router",
      [
        "react-native-video",
        {
          "enableNotificationControls": true,
          "enableBackgroundAudio": false,
          "enableADSExtension": true,
          "enableCacheExtension": false,
          "enableAndroidPictureInPicture": true,
          "androidExtensions": {
            "useExoplayerRtsp": true,
            "useExoplayerSmoothStreaming": true,
            "useExoplayerHls": true,
            "useExoplayerDash": true,
            "useExoplayerIMA": true
          }
        }
      ],
      "expo-asset",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "film-speak",
          "organization": "thouworks"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6c832a57-bb37-4208-9672-223a964aaa88"
      }
    },
    "owner": "thoulee",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/6c832a57-bb37-4208-9672-223a964aaa88",
      "requestHeaders": {
        "expo-runtime-version": "1.0.0",
        "expo-channel-name": "production"
      }
    }
  }
}